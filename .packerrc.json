{
  "entry": "index.ts",
  "source": "src",
  "dist": "dist",
  "output": {
    "amd": {
      "define": "",
      "id": ""
    },
    "dependencyMapMode": "mapDependency",
    "es2015": true,
    "es5": true,
    "minBundle": true,
    "format": "cjs",
    "imageInlineLimit": 1000000,
    "inlineStyle": false,
    "stylesDir": "styles"
  },
  "compiler": {
    "browserCompliant": false,
    "cliProject": true,
    "scriptPreprocessor": "typescript",
    "stylePreprocessor": "scss",
    "styleSupport": false
  },
  "assetPaths": [
    "src/assets"
  ],
  "copy": [
    "README.md",
    "LICENSE"
  ],
  "bundle": {
    "externals": [
      "handlebars/runtime",
      "path",
      "handlebars",
      "plugin-error",
      "through2",
      "chalk",
      "rollup-plugin-filesize",
      "rollup-plugin-progress",
      "rollup-plugin-img",
      "rollup-plugin-hbs",
      "rollup-plugin-babel",
      "rollup-plugin-typescript2",
      "typescript",
      "rollup-plugin-commonjs",
      "rollup-plugin-node-resolve",
      "rollup-plugin-ignore",
      "rollup-plugin-re",
      "autoprefixer",
      "postcss-image-inliner",
      "rollup-plugin-postcss",
      "rollup-plugin-ignore-import",
      "rollup",
      "fs",
      "gulp",
      "merge-stream",
      "gulp-file",
      "gulp-filter",
      "inquirer",
      "validate-npm-package-name",
      "validator/lib/isURL",
      "validator/lib/isEmail",
      "rollup-plugin-uglify",
      "lodash/merge",
      "gulp-clean",
      "karma-rollup-preprocessor",
      "rollup-plugin-istanbul",
      "child_process",
      "rollup-plugin-livereload",
      "rollup-plugin-serve",
      "karma",
      "gulp-chmod",
      "uglify-es",
      "@babel/runtime/helpers/toConsumableArray",
      "@babel/runtime/helpers/asyncToGenerator",
      "@babel/runtime/helpers/defineProperty",
      "@babel/runtime/regenerator"
    ],
    "globals": {},
    "mapExternals": false
  },
  "ignore": [],
  "pathReplacePatterns": [
    {
      "replace": "./config/replace-config",
      "test": "./config/base-config"
    }
  ],
  "testFramework": "jasmine",
  "watch": {
    "demoDir": "demo/watch",
    "helperDir": "demo/helper",
    "open": true,
    "port": 4000,
    "scriptDir": ".tmp",
    "serve": true
  },
  "license": {
    "banner": true,
    "thirdParty": {
      "fileName": "dependencies.txt",
      "includePrivate": false
    }
  }
}
