{
  "entry": "index.ts",
  "source": "src",
  "dist": "dist",
  "tmp": ".tmp",
  "output": {
    "amd": {
      "define": "",
      "id": ""
    },
    "dependencyMapMode": "map-dependency",
    "esnext": false,
    "es5": true,
    "minBundle": false,
    "format": "cjs",
    "imageInlineLimit": 1000000,
    "inlineStyle": false,
    "stylesDir": "styles",
    "namespace": "",
    "sourceMap": true
  },
  "compiler": {
    "buildMode": "node-cli",
    "scriptPreprocessor": "typescript",
    "stylePreprocessor": "scss",
    "styleSupport": false,
    "concurrentBuild": true
  },
  "assetPaths": [
    "src/assets"
  ],
  "copy": [
    "README.md",
    "LICENSE",
    "assets/**/{.*,*}"
  ],
  "bundle": {
    "externals": [
      "@babel/**",
      "autoprefixer",
      "chalk",
      "glob",
      "glob-to-regexp",
      "gulp",
      "gulp-chmod",
      "gulp-clean",
      "gulp-file",
      "handlebars",
      "inquirer",
      "karma-rollup-preprocessor",
      "less",
      "lodash*",
      "node-sass",
      "plugin-error",
      "postcss",
      "postcss-base64",
      "postcss-image-inliner",
      "rollup",
      "rollup-plugin-babel",
      "rollup-plugin-commonjs",
      "rollup-plugin-filesize",
      "rollup-plugin-hbs",
      "rollup-plugin-ignore",
      "rollup-plugin-ignore-import",
      "rollup-plugin-img",
      "rollup-plugin-istanbul",
      "rollup-plugin-livereload",
      "rollup-plugin-node-resolve",
      "rollup-plugin-postcss",
      "rollup-plugin-progress",
      "rollup-plugin-re",
      "rollup-plugin-serve",
      "rollup-plugin-typescript2",
      "rollup-pluginutils",
      "schema-inspector",
      "stylelint",
      "stylus",
      "through2",
      "tslib",
      "tslint",
      "uglify-es",
      "validate-npm-package-name",
      "validator/**",
      "path",
      "fs",
      "child_process"
    ],
    "globals": {},
    "mapExternals": false
  },
  "ignore": [],
  "replacePatterns": [],
  "testFramework": "jasmine",
  "watch": {
    "demoDir": "demo/watch",
    "helperDir": "demo/helper",
    "serveDir": [],
    "open": true,
    "port": 4000,
    "serve": true
  },
  "license": {
    "banner": true,
    "thirdParty": {
      "fileName": "dependencies.txt",
      "includePrivate": false
    }
  }
}
