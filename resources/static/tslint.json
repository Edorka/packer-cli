{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {
  },
  "rules": {
    "indent": [
      true,
      "spaces",
      2
    ],
    "quotemark": [
      true,
      "single"
    ],
    "trailing-comma": false,
    "object-literal-sort-keys": false,
    "interface-name": [true, "never-prefix"]
  },
  "rulesDirectory": []
}
