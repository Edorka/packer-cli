{
  "name": "packer-cli",
  "version": "2.9.2",
  "description": "Full-fledged CLI tool to generate and package library Node modules compliant with Browser and NodeJS. Packer CLI support all modern style, unit test and script transpiler tools.",
  "keywords": [
    "Library CLI",
    "Node Library",
    "NPM Library",
    "Browser Lib",
    "Lib CLI"
  ],
  "scripts": {
    "lint": "packer lint",
    "lint:style": "packer lint --style",
    "lint:script": "packer lint --script",
    "build": "packer build",
    "watch": "packer watch",
    "test": "packer test --watch",
    "test:coverage": "packer test --coverage --watch",
    "test:ci": "packer test",
    "test:coverage:ci": "packer test --coverage",
    "clean": "packer clean",
    "preversion": "npm run build",
    "postversion": "git push && git push --tags",
    "prerelease": "npm run build",
    "release": "npm publish dist"
  },
  "author": "Yohan Gomez <yohan.gz@hotmail.com>",
  "repository": "https://github.com/yohangz/packer-cli.git",
  "license": "MIT",
  "homepage": "https://github.com/yohangz/packer-cli",
  "dependencies": {
    "chalk": "^2.4.1",
    "glob": "^7.1.3",
    "glob-to-regexp": "^0.4.0",
    "gulp": "^4.0.0",
    "gulp-add": "^0.1.0",
    "gulp-chmod": "^2.0.0",
    "gulp-clean": "^0.4.0",
    "handlebars": "^4.0.12",
    "inquirer": "^6.2.0",
    "karma-rollup-preprocessor": "^6.1.1",
    "lodash": "^4.17.10",
    "packer-cli": "^2.9.2",
    "plugin-error": "^1.0.1",
    "rollup": "^0.67.3",
    "rollup-plugin-babel": "^4.0.3",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-filesize": "^5.0.1",
    "rollup-plugin-hbs": "^1.2.0",
    "rollup-plugin-ignore": "^1.0.3",
    "rollup-plugin-ignore-import": "^1.1.0",
    "rollup-plugin-img": "^1.1.0",
    "rollup-plugin-istanbul": "^2.0.1",
    "rollup-plugin-json": "^3.1.0",
    "rollup-plugin-livereload": "^0.6.0",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-postcss": "^1.6.2",
    "rollup-plugin-re": "^1.0.7",
    "rollup-plugin-serve": "^0.6.0",
    "rollup-plugin-typescript2": "^0.18.0",
    "rollup-pluginutils": "^2.3.3",
    "schema-inspector": "^1.6.8",
    "terser": "^3.10.11",
    "through2": "^3.0.0",
    "tslib": "^1.9.3",
    "typescript": "^3.1.1",
    "validate-npm-package-name": "^3.0.0",
    "validator": "^10.7.1"
  },
  "devDependencies": {
    "@babel/code-frame": "^7.0.0",
    "@babel/core": "^7.1.6",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.1.6",
    "@babel/register": "^7.0.0",
    "@babel/runtime": "^7.1.0",
    "@types/gulp": "^4.0.5",
    "@types/handlebars": "^4.0.39",
    "@types/inquirer": "^0.0.43",
    "@types/jest": "^23.3.10",
    "@types/karma": "^3.0.1",
    "@types/lodash": "^4.14.117",
    "@types/plugin-error": "^0.1.0",
    "@types/through2": "^2.0.34",
    "@types/validator": "^9.4.3",
    "chokidar": "^2.0.4",
    "eslint": "^5.9.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "jest": "^23.6.0",
    "ts-jest": "^23.10.5",
    "tslint": "^5.11.0"
  },
  "private": false,
  "bin": {
    "packer": "bin/packer-cli.js"
  }
}
